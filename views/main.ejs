<%- include ./part/header.ejs %>

<h1 class="pool">Pool matches</h1>
<% games.forEach(game => { %>
	<a href="/score/<%= game.game_id %>">
		<div class="match">
			<div>
				<h3><%= game.leftScore %></h3>
				<p><%= game.leftTeam %></p>
			</div>
			<p>vs</p>
			<div>
				<h3><%= game.rightScore %></h3>
				<p><%= game.rightTeam %></p>
			</div>
		</div>
	</a>
<% }); %>

<%- include ./part/footer.ejs %>
