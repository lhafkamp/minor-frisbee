<%- include ./part/header.ejs %>

<h1 class="pool">Pool matches</h1>

<% games.forEach(game => { %>
	<a href="/score/<%= game.game_id %>">
		<section class="match">
			<article>
				<p><%= game.leftScore %></p>
				<p><%= game.leftTeam %></p>
			</article>
			<p class="vs">vs</p>
			<article>
				<p><%= game.rightScore %></p>
				<p><%= game.rightTeam %></p>
			</article>
		</section>
	</a>
<% }); %>

<%- include ./part/footer.ejs %>
