<%- include ./part/header.ejs %>

<h1 class="pool">Pool matches</h1>

<% games.forEach(game => { %>
	<a href="/score/<%= game.game_id %>">
		<div class="match">
			<div>
				<p><%= game.leftScore %></p>
				<p><%= game.leftTeam %></p>
			</div>
			<p class="vs">vs</p>
			<div>
				<p><%= game.rightScore %></p>
				<p><%= game.rightTeam %></p>
			</div>
		</div>
	</a>
<% }); %>

<%- include ./part/footer.ejs %>
